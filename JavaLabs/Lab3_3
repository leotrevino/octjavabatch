
import java.util.Arrays;


public class JavaLab3_3 {

	
	public static void main(String[] args)
	{
		// Integer Array containing 10 elements.
		int[] array = new int[] { 5, 1, -9, 10, 200, 3, 33, 41, 2, -90 };

		// Displaying elements of original array
		System.out.print("Elements of original array: ");
		for (int i = 0; i < array.length; i++) {
			System.out.print(array[i] + " ");
		}

		// Using Arrays.sort() method to sort array elements in ascending order.
		Arrays.sort(array);

		System.out.println();

		System.out.println("Elements of array sorted in ascending order : " + Arrays.toString(array));
	}
}
